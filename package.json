{
  "name": "socketio-msg-server",
  "version": "1.0.0",
  "description": "A client-server application using socket.io that receives a message and sends its characters back one by one in an interval",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "develop": "cross-env NODE_ENV=development node src/server/server.js",
    "debug": "cross-env NODE_ENV=development DEBUG=engine,socket.io:client* node src/server/server.js",
    "start": "cross-env NODE_ENV=production node src/server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Denis Dincer",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "webpack-dev-middleware": "^5.2.1",
    "webpack-remove-debug": "^0.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.4",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "babel-loader": "^8.2.3",
    "@babel/preset-env": "^7.16.0",
    "css-loader": "^6.5.1",
    "cross-env": "^7.0.3"
  }
}
